# Model
model:
  rest_name: tagvalue
  resource_name: tagvalues
  entity_name: TagValue
  package: tagle
  group: core/tag
  description: Represents all values associated to tag key.

# Attributes
attributes:
  v1:
  - name: key
    description: The requested key.
    type: string
    exposed: true
    read_only: true
    autogenerated: true

  - name: values
    description: List of all values.
    type: list
    exposed: true
    subtype: string
    read_only: true
    autogenerated: true
